<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!--
         Used in Action Bar, and in AndroidManifest to tell the device the name of this app.
         It's to keep this short, so your launcher icon isn't displayed with "The greatest Wea"
         or something similar.
    -->
    <string name="app_name">Hawa Rayi</string>

    <!--
         By convention, "action" denotes that this String will be used as the label for an Action,
         typically from the action bar.  The ActionBar is limited real estate, so shorter is better.
    -->
    <string name="action_settings">Sazlawlar</string>
    <string name="action_share">Bo\'lisiw</string>
    <string name="action_refresh">Jan\'alaw</string>

    <string name="title_activity_detail">Toli\'q</string>
    <string name="title_activity_settings">Sazlawlar</string>

    <!-- Label for the location preference [CHAR LIMIT=30] -->
    <string name="pref_location_label">Qala</string>

    <!-- Notification Format -->
    <string name="format_notification">
        Forecast: <xliff:g id="condition">%1$s</xliff:g> High: <xliff:g id="high">%2$s</xliff:g>
        Low: <xliff:g id="low">%3$s</xliff:g>
    </string>

     <!-- Strings related to Notification Enabled preference -->
     <string name="pref_enable_notifications_label">Hawa Rayi xabarlandi\'ri\'wi\'</string>

     <string name="pref_enable_notifications_true">Qosi\'lg\'an</string>
     <string name="pref_enable_notifications_false">O\'shirilgen</string>

    <!-- Language-specific constants -->
    <string name="today">Bu\'gu\'n</string>

    <!-- For labelling tomorrow's forecast [CHAR LIMIT=15] -->
    <string name="tomorrow">Yerten\'</string>

    <!-- Windspeed formats -->
    <!-- Wind in kph [CHAR LIMIT=25] -->
    <string name="format_wind_kmh">
        Samal: <xliff:g id="speed">%1$1.0f</xliff:g> m/s, <xliff:g id="direction">%2$s</xliff:g>
    </string>
    <string name="wind_direction_north">arqa</string>
    <string name="wind_direction_north_east">arqa-shi\'g\'i\'s</string>
    <string name="wind_direction_east">shi\'g\'i\'s</string>
    <string name="wind_direction_south_east">qubla-shi\'g\'i\'s</string>
    <string name="wind_direction_south">qubla</string>
    <string name="wind_direction_south_west">qubla-bati\'s</string>
    <string name="wind_direction_west">bati\'s</string>
    <string name="wind_direction_north_west">arqa-bati\'s</string>

    <!-- Pressure format CHAR LIMIT=25] -->
    <string name="format_pressure">Basi\'m: <xliff:g id="pressure">%1.0f</xliff:g> mm. si\'nap bag\'anasi\'</string>

    <!-- Humidity format CHAR LIMIT=25]-->
    <string name="format_humidity">I\'g\'alli\'q: <xliff:g id="humidity">%1.0f</xliff:g>%%</string>

    <!-- Share format [CHAR LIMIT=NONE]-->
    <string name="format_share">
        Weather for <xliff:g id="date">%1$s</xliff:g> in <xliff:g id="location">%2$s</xliff:g>:
        A high of <xliff:g id="high">%3$s</xliff:g> and low of
        <xliff:g id="low">%4$s</xliff:g>
    </string>

